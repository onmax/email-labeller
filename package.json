{
  "name": "email-labeller",
  "type": "module",
  "version": "0.2.1-alpha.1",
  "packageManager": "pnpm@10.0.0",
  "description": "AI-powered email classification for Gmail",
  "author": "onmax",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onmax/email-labeller.git"
  },
  "keywords": [
    "email",
    "gmail",
    "ai",
    "classification",
    "labeller",
    "automation"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "bin": {
    "email-labeller": "./dist/cli/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/cli/index.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "release": "changelogen --release --push && pnpm publish --access public",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "ai": "^6.0.5",
    "citty": "^0.1.6",
    "consola": "^3.4.2",
    "google-auth-library": "^10.5.0",
    "googleapis": "^169.0.0",
    "pathe": "^2.0.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.3",
    "@types/node": "^25.0.3",
    "bumpp": "^10.3.2",
    "changelogen": "^0.6.2",
    "eslint": "^9.28.0",
    "eslint-plugin-format": "^1.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
